import java.util.*;
public class zeromatrix1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int m=4;
		int n=4;
		int ar[][]=new int[m][n];
		for(int i=0;i<m;i++) {
			for(int j=0;j<n;j++) {
				ar[i][j]=sc.nextInt();
			}
		}
for(int i=0;i<m;i++) {
	int k=0;
	for(int j=0;j<n;j++) {
		if(ar[i][j]==0) {
			ar[i][j]=0;
			break;
			
		}
		while(k<m) {
			ar[i][k]=0;
		}
	}
}
	}

}
